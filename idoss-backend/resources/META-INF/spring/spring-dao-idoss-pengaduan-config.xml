<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--Pengaduan-->
    <bean id="pRootCausedDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.pengaduan.dao.PRootCausedDAO"/>
    </bean>
    <bean id="pTypeDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.pengaduan.dao.PTypeDAO"/>
    </bean>
    <bean id="tPenangananGangguanDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.pengaduan.dao.TPenangananGangguanDAO"/>
    </bean>
    <bean id="vHrEmployeePelaksanaDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.pengaduan.dao.VHrEmployeePelaksanaDAO"/>
    </bean>
    <bean id="pTypeRootCausedDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.pengaduan.dao.PTypeRootCausedDAO"/>
    </bean>

</beans>