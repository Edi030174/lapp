<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--permohonan-->
    <bean id="tPelaksanaanDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.permohonan.dao.TPelaksanaanDAO"/>
    </bean>
    <bean id="tPermohonanDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.permohonan.dao.TPermohonanDAO"/>
    </bean>
    <bean id="tVerifikasiDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.permohonan.dao.TVerifikasiDAO"/>
    </bean>
    <bean id="tHolidayDAO" class="com.xsis.springframework.orm.ibatis3.support.MapperFactoryBean">
        <property name="sqlSessionFactory" ref="sqlSessionFactory"/>
        <property name="mapperInterface" value="net.lintasarta.permohonan.dao.THolidayDAO"/>
    </bean>
</beans>