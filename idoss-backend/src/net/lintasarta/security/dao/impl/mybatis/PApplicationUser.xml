<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="net.lintasarta.security.dao.PApplicationUserDAO">
    <select id="getEmployeeNoByUserName" parameterType="string" resultType="string">
        select a.EMPLOYEE_NO from  P_APPLICATION_USER a
        where a.USER_NAME = #{userName}
    </select>
</mapper>